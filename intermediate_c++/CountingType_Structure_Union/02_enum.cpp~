// Course title: // enum

#include <iostream>
//#include <curses.h>

using namespace std;



int main()
{	int x;
	enum Data {A = 3, B = 4 , C = 2 };	
	
	x = A * B * C;
	cout << x <<endl;             // 24

	return 0;
}

