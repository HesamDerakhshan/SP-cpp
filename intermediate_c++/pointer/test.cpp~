// Course title: Reference in function  

#include <iostream>
using namespace std;


int &min(int &, int &);


int main()
{
	int x = 3;
	int y = 5;
	
	
	min(x,y) = 8;  // x = 8 function name written in left side when function's return type is reference 
	
	cout << a[0] << endl;
	cout << a[1] << endl;
	cout << a[2] << endl;
		
  return 0;
}

int &f(int i){
	return a[i];
} 
