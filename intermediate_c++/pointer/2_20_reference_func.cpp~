// Course title: Reference in function  

#include <iostream>
using namespace std;

void f(int &);

char s[20] = "Hesam Derakhshan.";
char &f(int);

int main()
{
	f(a7) = ',';	

	cout << s << endl;
		
  return 0;
}

void f(int &r){
	r++;
}
