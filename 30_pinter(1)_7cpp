#include <iostream>
using namespace std;
int main()
{    // pointer to pointer
	// q -> p -> x=19
	int x = 19;	
	int *p, **q, ***t;  
	
	p = &x;
	
	q = &p;
	
	t = &q;  
	 
	cout << "*p: "    << *p<<endl;  // =19
	cout << "**q : "  << **q<<endl;   // =19
	cout << "***t : "  << ***t<<endl;   // =19



return 0;
}

